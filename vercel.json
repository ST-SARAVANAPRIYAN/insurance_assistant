{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "assets/**",
      "use": "@vercel/static"
    },
    {
      "src": "data/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/chat",
      "dest": "/chat-interface.html"
    },
    {
      "src": "/claims",
      "dest": "/claim-assessment.html"
    },
    {
      "src": "/plans",
      "dest": "/plan-management-interface.html"
    },
    {
      "src": "/(.*\\.(html|css|js|json|svg|png|jpg|jpeg|gif|ico))",
      "dest": "/$1"
    },
    {
      "src": "/",
      "dest": "/index.html"
    }
  ]
}
