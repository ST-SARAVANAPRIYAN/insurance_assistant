{
  "version": 2,
  "builds": [
    {
      "src": "chat-server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/chat-server.js"
    },
    {
      "src": "/chat",
      "dest": "/chat-server.js"
    },
    {
      "src": "/claims",
      "dest": "/chat-server.js"
    },
    {
      "src": "/plans",
      "dest": "/chat-server.js"
    },
    {
      "src": "/chat-interface.html",
      "dest": "/chat-server.js"
    },
    {
      "src": "/claim-assessment.html",
      "dest": "/chat-server.js"
    },
    {
      "src": "/plan-management-interface.html",
      "dest": "/chat-server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/chat-server.js"
    }
  ],
  "functions": {
    "chat-server.js": {
      "maxDuration": 30
    }
  }
}
