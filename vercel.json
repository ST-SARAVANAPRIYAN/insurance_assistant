{
  "version": 2,
  "functions": {
    "chat-server.js": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/chat-server.js"
    },
    {
      "source": "/chat",
      "destination": "/chat-server.js"
    },
    {
      "source": "/claims",
      "destination": "/chat-server.js"
    },
    {
      "source": "/plans",
      "destination": "/chat-server.js"
    },
    {
      "source": "/chat-interface.html",
      "destination": "/chat-server.js"
    },
    {
      "source": "/claim-assessment.html",
      "destination": "/chat-server.js"
    },
    {
      "source": "/plan-management-interface.html",
      "destination": "/chat-server.js"
    },
    {
      "source": "/(.*)",
      "destination": "/chat-server.js"
    }
  ]
}
